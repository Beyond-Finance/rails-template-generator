{
  "name": "rails_template_generator",
  "private": true,
  "dependencies": {
    "@rails/activestorage": "^6.0.0",
    "@rails/ujs": "^6.0.0",
    "@rails/webpacker": "5.3.0",
    "bootstrap": "^4.5.0",
    "dns-packet": "^1.3.4",
    "glob-parent": "^5.1.2",
    "hosted-git-info": "^2.8.9",
    "jquery": "^3.5.1",
    "jquery-ujs": "^1.2.2",
    "lodash": "^4.17.21",
    "normalize-url": "^4.5.1",
    "popper.js": "^1.16.1",
    "stylelint": "^13.6.1",
    "trim": "^0.0.3",
    "trim-newlines": "^3.0.1",
    "url-parse": "^1.5.1",
    "ws": "^7.4.6"
  },
  "resolutions": {
    "elliptic": "6.5.4",
    "is-svg": "4.2.2",
    "ssri": "8.0.1"
  },
  "version": "0.1.0",
  "devDependencies": {
    "babel-jest": "^26.1.0",
    "jest": "^26.1.0",
    "jest-junit": "^11.0.1",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/app/javascript/tests"
    ],
    "verbose": true,
    "testURL": "http://localhost",
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/app/javascript"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "transform": {
      "^.+\\.js(x)?$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "scripts": {
    "css-fix": "stylelint --fix app/**/*.*css",
    "test": "jest --config package.json --passWithNoTests"
  }
}
