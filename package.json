{
  "name": "rails_template_generator",
  "private": true,
  "dependencies": {
    "@rails/activestorage": "^6.1.4",
    "@rails/ujs": "^6.1.4",
    "@rails/webpacker": "5.4.0",
    "bootstrap": "^4.5.0",
    "dns-packet": "^5.3.0",
    "hosted-git-info": "^4.0.2",
    "jquery": "^3.5.1",
    "jquery-ujs": "^1.2.3",
    "lodash": "^4.17.21",
    "normalize-url": "^7.0.0",
    "popper.js": "^1.16.1",
    "stylelint": "^13.13.1",
    "trim": "^1.0.1",
    "trim-newlines": "^4.0.2",
    "url-parse": "^1.5.2",
    "ws": "^7.5.3"
  },
  "resolutions": {
    "color-string": "1.5.5",
    "elliptic": "6.5.4",
    "glob-parent": "6.0.0",
    "is-svg": "4.3.1",
    "ssri": "8.0.1"
  },
  "version": "0.1.0",
  "devDependencies": {
    "babel-jest": "^27.0.6",
    "jest": "^27.0.6",
    "jest-junit": "^12.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/app/javascript/tests"
    ],
    "verbose": true,
    "testURL": "http://localhost",
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/app/javascript"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "transform": {
      "^.+\\.js(x)?$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "scripts": {
    "css-fix": "stylelint --fix app/**/*.*css",
    "test": "jest --config package.json --passWithNoTests"
  }
}
